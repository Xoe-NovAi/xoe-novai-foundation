      {
        "id": 11,
        "title": "Success Rate by Task Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(openpipe_requests_total{success=\"true\"}[5m]) / rate(openpipe_requests_total[5m]) * 100",
            "legendFormat": "Overall Success Rate"
          },
          {
            "expr": "rate(openpipe_requests_total{task_type=\"code_generation\", success=\"true\"}[5m]) / rate(openpipe_requests_total{task_type=\"code_generation\"}[5m]) * 100",
            "legendFormat": "Code Generation Success Rate"
          },
          {
            "expr": "rate(openpipe_requests_total{task_type=\"research\", success=\"true\"}[5m]) / rate(openpipe_requests_total{task_type=\"research\"}[5m]) * 100",
